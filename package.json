{
  "name": "react-redux-ts-starter",
  "version": "0.0.1",
  "description": "Starter kit for React, Webpack (with Hot Module Replacement), Typescript, Redux, and Babel.",
  "keywords": [
    "react",
    "redux",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "author": "Luke Brown",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/worbunkel/react-redux-ts-starter.git"
  },
  "bugs": {
    "url": "https://github.com/worbunkel/react-redux-ts-starter/issues"
  },
  "homepage": "https://github.com/worbunkel/react-redux-ts-starter",
  "scripts": {
    "build": "npm run clean-dist && webpack -p --config=configs/webpack/prod.js && cd dist && zip -r -X deployableZip.zip index.html js",
    "clean-dist": "rm -f -r dist",
    "gen": "run(){ node scripts/generate/generate.js $* && npm run lint; }; run",
    "lint": "npm run prettier && npm run lint-ts && npm run lint-sass",
    "lint-ts": "eslint ./src/**/*.ts ./src/**/*.tsx",
    "lint-sass": "stylelint ./src/**/*.scss",
    "prettier": "prettier --config .prettierrc --write \"src/**/*.{ts,tsx,scss}\" --loglevel error",
    "start": "npm run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "start-prod": "npm run build && node express.js",
    "test": "jest --watchAll --coverage --config=configs/jest.json",
    "test-once": "jest --coverage --config=configs/jest.json",
    "webpack": "webpack --config=configs/webpack/dev.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run webpack && npm run test-once",
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.121",
    "@types/node": "^10.7.1",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.1",
    "@types/react-redux": "^7.0.1",
    "@types/react-router": "^4.4.4",
    "@types/react-router-dom": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^1.3.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-typescript": "^0.14.0",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.2",
    "ignore-loader": "^0.1.2",
    "image-webpack-loader": "^4.4.0",
    "jest": "^23.5.0",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.8.1",
    "react-hot-loader": "^4.3.4",
    "reactotron-react-js": "^2.1.1",
    "reactotron-redux": "^2.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.1.3",
    "stylelint-webpack-plugin": "^0.10.5",
    "typescript": "^3.3.3",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "compression": "^1.7.3",
    "connected-react-router": "^6.3.1",
    "dayjs": "^1.7.7",
    "graphql-request": "^1.8.2",
    "history": "^4.7.2",
    "localforage": "^1.7.3",
    "lodash": "^4.17.10",
    "mdi-react": "^4.4.0",
    "mocker-data-generator": "^2.6.6",
    "number-abbreviate": "^2.0.0",
    "react": "^16.8.1",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0"
  }
}
